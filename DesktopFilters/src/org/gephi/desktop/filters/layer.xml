<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="org-gephi-desktop-filters-FiltersAction.instance">
                <attr name="component" methodvalue="org.gephi.desktop.filters.FiltersTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="org.gephi.desktop.filters.Bundle#CTL_FiltersAction"/>
                <attr name="iconBase" stringvalue="org/gephi/desktop/filters/resources/icon.png"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="FiltersAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-gephi-desktop-filters-FiltersAction.instance"/>
                <attr name="position" intvalue="400"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="FiltersTopComponent.settings" url="FiltersTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="filtersmode">
                <file name="FiltersTopComponent.wstcref" url="FiltersTopComponentWstcref.xml"/>
            </folder>
            <file name="filtersmode.wsmode" url="filtersWsmode.xml"/>
        </folder>
    </folder>
</filesystem>
