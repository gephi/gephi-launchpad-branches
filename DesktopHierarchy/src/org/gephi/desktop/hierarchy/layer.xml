        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="HierarchyTopComponent.settings" url="HierarchyTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="graphmode">
