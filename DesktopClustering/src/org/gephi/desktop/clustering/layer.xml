<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="org-gephi-desktop-clustering-ClusteringAction.instance">
                <attr name="component" methodvalue="org.gephi.desktop.clustering.ClusteringTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="org.gephi.desktop.clustering.Bundle#CTL_ClusteringAction"/>
                <attr name="iconBase" stringvalue="org/gephi/desktop/clustering/resources/icon.png"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="ClusteringAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-gephi-desktop-clustering-ClusteringAction.instance"/>
                <attr name="position" intvalue="100"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="ClusteringTopComponent.settings" url="ClusteringTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="rankingmode">
                <file name="ClusteringTopComponent.wstcref" url="ClusteringTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
