        <folder name="Window">
            <file name="org-gephi-desktop-similarity-SimilarityAction.instance">
                <attr name="component" methodvalue="org.gephi.desktop.similarity.SimilarityTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="org.gephi.desktop.similarity.Bundle#CTL_SimilarityAction"/>
                <attr name="iconBase" stringvalue="org/gephi/desktop/similarity/resources/icon.png"/>
