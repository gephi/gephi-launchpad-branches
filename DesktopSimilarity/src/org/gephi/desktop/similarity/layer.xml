            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="SimilarityAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-gephi-desktop-similarity-SimilarityAction.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="SimilarityTopComponent.settings" url="SimilarityTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="filtersmode">
                <file name="SimilarityTopComponent.wstcref" url="SimilarityTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
