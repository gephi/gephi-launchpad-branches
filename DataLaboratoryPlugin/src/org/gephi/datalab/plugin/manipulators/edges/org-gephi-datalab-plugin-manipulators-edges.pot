msgid "OpenInEditEdgeWindow.name"
msgstr "Edit edge"

msgid "OpenInEditEdgeWindow.name.multiple"
msgstr "Edit all edges"

msgid "OpenInEditEdgeWindow.description"
msgstr ""
"Opens edit window to edit the selected edge. It allows to edit color of the "
"edge and also the attributes."

msgid "OpenInEditEdgeWindow.description.multiple"
msgstr ""
"Opens edit window to edit the selected edges. Fields will be blank at first. "
"It allows to edit color of the edge and also the attributes."

msgid "SelectSourceOnGraph.name"
msgstr "Select source node on graph view"

msgid "SelectTargetOnGraph.name"
msgstr "Select target node on graph view"

msgid "SelectNodesOnTable.name"
msgstr "Select source and target on nodes table"

msgid "DeleteEdges.name.single"
msgstr "Delete"

msgid "DeleteEdges.name.multiple"
msgstr "Delete all"

msgid "DeleteEdges.confirmation.message"
msgstr "Are you sure you want to delete the edge(s)?"

msgid "DeleteEdgesWithNodes.name.single"
msgstr "Delete edge with nodes..."

msgid "DeleteEdgesWithNodes.name.multiple"
msgstr "Delete all edges with nodes..."

msgid "ClearEdgesData.name.single"
msgstr "Clear edge data..."

msgid "ClearEdgesData.name.multiple"
msgstr "Clear all edges data..."

msgid "ClearEdgesData.description"
msgstr "Clears the desired columns data of the selected edge(s)"

msgid "ClearEdgesData.ui.description"
msgstr "Choose columns to clear data:"

msgid "CopyEdgeDataToOtherEdges.name"
msgstr "Copy edge data to the other selected edges..."

msgid "CopyEdgeDataToOtherEdges.description"
msgstr ""
"Copies the desired columns of the selected edge to the other selected edges"

msgid "CopyEdgeDataToOtherEdges.ui.description"
msgstr "Choose columns to copy data:"
