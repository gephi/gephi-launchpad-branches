<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="org-gephi-desktop-context-ContextAction.instance">
                <attr name="component" methodvalue="org.gephi.desktop.context.ContextTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="org.gephi.desktop.context.Bundle#CTL_ContextAction"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="ContextAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-gephi-desktop-context-ContextAction.instance"/>
                <attr name="position" intvalue="200"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="ContextTopComponent.settings" url="ContextTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="contextmode">
                <file name="ContextTopComponent.wstcref" url="ContextTopComponentWstcref.xml"/>
            </folder>
             <file name="contextmode.wsmode" url="contextmodeWsmode.xml"/>
        </folder>
    </folder>
</filesystem>
