<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="org-gephi-desktop-layout-LayoutAction.instance">
                <attr name="component" methodvalue="org.gephi.desktop.layout.LayoutTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="org.gephi.desktop.layout.Bundle#CTL_LayoutAction"/>
                <attr name="iconBase" stringvalue="org/gephi/desktop/layout/resources/icon.png"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="LayoutAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-gephi-desktop-layout-LayoutAction.instance"/>
                <attr name="position" intvalue="700"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="LayoutTopComponent.settings" url="LayoutTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <file name="layoutmode.wsmode" url="layoutWsmode.xml"/>
            <folder name="layoutmode">
                <file name="LayoutTopComponent.wstcref" url="LayoutTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
